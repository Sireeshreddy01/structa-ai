# Server
AI_HOST=0.0.0.0
AI_PORT=8000
AI_WORKERS=4
AI_DEBUG=false

# Redis
AI_REDIS_URL=redis://localhost:6379

# Backend API
AI_BACKEND_URL=http://localhost:3000
AI_BACKEND_API_KEY=

# Storage
AI_STORAGE_TYPE=local
AI_STORAGE_PATH=/tmp/structa-ai
AI_S3_BUCKET=structa-documents
AI_S3_ENDPOINT=http://localhost:9000

# AI Models
AI_OCR_ENGINE=tesseract
AI_LAYOUT_MODEL=lp://PubLayNet/faster_rcnn_R_50_FPN_3x
AI_TABLE_ENGINE=img2table

# Processing
AI_MAX_IMAGE_SIZE=4096
AI_JPEG_QUALITY=85
AI_CONFIDENCE_THRESHOLD=0.5
